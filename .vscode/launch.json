{
    "version": "0.2.0",
    "configurations": [{
        "type": "node",
        "request": "launch",
        "name": "Launch Program",
        "runtimeVersion": "10.9.0",
        "program": "${workspaceFolder}/index.mjs",
        "envFile": "${workspaceFolder}/.env",
        "env": {
            "NODE_ENV": "development",
            "DEBUG": "api.yicf.org:*",
        },
        "runtimeArgs": [
            "--nolazy",
            "--experimental-modules"
        ],
        "outputCapture": "std"
    }]
}